{
    "manifest_version": 2,

    "name": "Track This Element",
    "description": "Receive notifications when changes are made to webpages you are following.",
    "version": "0.0.1",
    "permissions": [
        "contextMenus", "storage"
    ],
    "icons" : {
        "16": "icon16.png"
    },
    "background": {
        "scripts" : [
            "js/context.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}