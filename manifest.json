{
    "manifest_version": 2,

    "name": "Track This Element",
    "description": "Receive notifications when changes are made to webpages you are following.",
    "version": "0.0.1",
    "permissions": [
        "contextMenus", 
        "storage",
        "tabs"
    ],
    "background": {
        "scripts": ["lib/jquery-2.1.0.js", "js/background.js"]
    },
    "icons" : {
        "16": "icon16.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches" : [
                "<all_urls>"
            ], "js": [
                "lib/jquery-2.1.0.js",
                "js/context.js"
            ]
        }
    ]
}
